<div class="container">
  <h1>Transactions</h1>

  <div id="transactions">
    <% @transactions.each do |transaction| %>
      <%= render item %>
      <p>
        <%= link_to "Show", transaction %>
        <%= link_to "Edit", edit_transaction_path(transaction) %>
        <%= link_to "Delete", delete_transaction_path(transaction) %>
      </p>
    <% end %>
  </div>

  <%= link_to "Add Transaction", new_transaction_path %>
</div>
